LowVestTrainFunc = function()
    game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
    task.wait()
    for i,v in next, game.Players.LocalPlayer.Backpack:GetChildren() do
    if v:isA("Tool") and v.Name == "25KG Vest" then
        game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
        game:GetService("Players").LocalPlayer.Character:FindFirstChild("25KG Vest"):Activate()
        game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
        task.wait()
    local vestN = "25KG Vest"
    local shakeN = "Vanilla Protein Shake"
    local weightN = "25KG Dumbbell"
    
    local plr = game.Players.LocalPlayer

    while LowVestTraining and task.wait() do
        pcall(function()
            vest = plr.Backpack:FindFirstChild(vestN) or plr.Character:FindFirstChild(vestN)
            shake = plr.Backpack:FindFirstChild(shakeN) or plr.Character:FindFirstChild(shakeN)
            weight = plr.Backpack:FindFirstChild(weightN) or plr.Character:FindFirstChild(weightN)
        
            if StaminaCheck() then
               stamcheck = true
               repeat
                   local check = plr.Character:FindFirstChild("Stamina").Value
                   local MaxStamina = plr.Character:FindFirstChild("Stamina"):GetAttribute("Max")
                   task.wait()
               until check >= MaxStamina or LowVestTraining == false
               stamcheck = false
            end
            
            if vest.Parent == plr.Backpack and vest.Parent ~= plr.Character and not plr.Character:FindFirstChild(vestN):IsA("Model") and plr.Character:FindFirstChild(shakeN) == nil then
               plr.Character.Humanoid:EquipTool(plr.Backpack:FindFirstChild(vestN))
               task.wait(1)
                plr.Character:FindFirstChild(vestN):Activate()
                task.wait(0.5)
            elseif plr.Character:FindFirstChild(vestN) and plr.Character:FindFirstChild(vestN):IsA("Model") and plr.Character:FindFirstChild(shakeN) == nil then
                if weight.Parent == plr.Backpack and plr.Character:FindFirstChild(shakeN) == nil then
                   plr.Character.Humanoid:EquipTool(weight)
                   task.wait(1)
                elseif weight.Parent == plr.Character and plr.Character:FindFirstChild(shakeN) == nil then
                    weight:Activate()
                end
            end
        end)
    end
end
end
end

HitKickFunc = function()
    while HitKick do task.wait()
        pcall(function()

        local plr = game.Players.LocalPlayer
    local hum = plr.Character:WaitForChild("Humanoid") 
    if hum.Health == hum.MaxHealth - 10 then 
        plr:kick("Hit")
    end
    end)
    end
        task.wait()
        local plr = game.Players.LocalPlayer
        local hum = plr.Character:WaitForChild("Humanoid") 
        if hum.Health == hum.MaxHealth - 10 then 
        task.wait()
end
end

WingChunFunc = function()
    local function GetWingChuns()
        local h = game:GetService("Players").LocalPlayer.Character.Humanoid
            h:MoveTo(Vector3.new(392, 97, 2489))
            h.MoveToFinished:wait()
        local chingchun = {}
        
       for i,v in pairs(TrainingFolder:GetChildren()) do
            if v.Name == "WingChun" then
                table.insert(chingchun,v) 
            end
       end
       
       return chingchun
    end
    
    local WingChuns = GetWingChuns()
    
    while AutoWingChun and task.wait() do 
        if HPCheck() then
           stamcheck = true
           
           repeat
               local check = plr.Character:FindFirstChild("Stamina").Value
               local MaxStamina = plr.Character:FindFirstChild("Stamina"):GetAttribute("Max")
               local hum = plr.Character:WaitForChild("Humanoid")
               local tool
               
               if tool == nil then
                  if plr.Character:FindFirstChild("Situps") then
                      tool = plr.Character:FindFirstChild("Situps")
                  else
                      tool = plr.Backpack:FindFirstChild("Situps")
                  end
               end
               
               if hum.Health <= hum.MaxHealth then
                   if StaminaCheck() then
                       stamcheck = true
                       repeat
                           local check = plr.Character:FindFirstChild("Stamina").Value
                           local MaxStamina = plr.Character:FindFirstChild("Stamina"):GetAttribute("Max")
                           task.wait()
                       until check >= MaxStamina or hum.Health >= hum.MaxHealth or AutoWingChun == false
                       stamcheck = false
                   end
                   
                   if tool.Parent == plr.Backpack and hum.Health <= hum.MaxHealth then
                       plr.Character.Humanoid:EquipTool(tool)
                       task.wait(1)
                    elseif tool.Parent == plr.Character and hum.Health <= hum.MaxHealth then
                        tool:Activate()
                        task.wait()
                    end
                end
                
               task.wait()
           until hum.Health >= hum.MaxHealth or AutoWingChun == false
           stamcheck = false
        end
        
        if plr.Backpack:FindFirstChild("Combat") and stamcheck == false then
            plr.Backpack:FindFirstChild("Combat").Parent = plr.Character
            task.wait(0.5)
        elseif plr.Character:FindFirstChild("Combat") and stamcheck == false then
            local vi = game:service'VirtualInputManager'
            vi:SendMouseButtonEvent(500, 500, 0, true, game, 1)
            task.wait()
        end
    end
end

EnduranceFarmFunc = function()
    while EnduranceFarm do task.wait(0.5)
        game:GetService("ReplicatedStorage").Communication.Events.RunEvent:FireServer("Running",true)
    
        if StaminaCheck() and stamcheck == false then
            game:GetService("ReplicatedStorage").Communication.Events.RunEvent:FireServer("Running",false)
            stamcheck = true
           repeat
               local check = plr.Character:FindFirstChild("Stamina").Value
               local MaxStamina = plr.Character:FindFirstChild("Stamina"):GetAttribute("Max")
               task.wait()
           until check >= MaxStamina or EnduranceFarm == false
           stamcheck = false
        end
    
    end
end

HitboxFunc = function()
    while Hitbox do task.wait(0.7) do
for i,v in next, game:GetService('Players'):GetPlayers() do
if v.Name ~= game:GetService('Players').LocalPlayer.Name then
v.Character.HumanoidRootPart.Size = Vector3.new(20,20,20)
v.Character.HumanoidRootPart.Transparency = 0.9
v.Character.HumanoidRootPart.BrickColor = BrickColor.new("Dark grey metallic")
v.Character.HumanoidRootPart.Material = "Neon"
v.Character.HumanoidRootPart.CanCollide = false 
end
end
end
end
for i,v in next, game:GetService('Players'):GetPlayers() do
if v.Name ~= game:GetService('Players').LocalPlayer.Name then
v.Character.HumanoidRootPart.Size = Vector3.new(2,2,2)
v.Character.HumanoidRootPart.Transparency = 0.9
v.Character.HumanoidRootPart.BrickColor = BrickColor.new("Dark grey metallic")
v.Character.HumanoidRootPart.Material = "Neon"
v.Character.HumanoidRootPart.CanCollide = false 
end
end
end

MediumVestTrainFunc = function()
    game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
    task.wait()
    for i,v in next, game.Players.LocalPlayer.Backpack:GetChildren() do
    if v:isA("Tool") and v.Name == "50KG Vest" then
        game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
        game:GetService("Players").LocalPlayer.Character:FindFirstChild("50KG Vest"):Activate()
        game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
        task.wait()

    local vestN = "50KG Vest"
    local shakeN = "Vanilla Protein Shake"
    local weightN = "25KG Dumbbell"
    
    local plr = game.Players.LocalPlayer
    
    while MediumVestTraining and task.wait() do
        pcall(function()
            vest = plr.Backpack:FindFirstChild(vestN) or plr.Character:FindFirstChild(vestN)
            shake = plr.Backpack:FindFirstChild(shakeN) or plr.Character:FindFirstChild(shakeN)
            weight = plr.Backpack:FindFirstChild(weightN) or plr.Character:FindFirstChild(weightN)
        
            if StaminaCheck() then
               stamcheck = true
               repeat
                   local check = plr.Character:FindFirstChild("Stamina").Value
                   local MaxStamina = plr.Character:FindFirstChild("Stamina"):GetAttribute("Max")
                   task.wait()
               until check >= MaxStamina or MediumVestTraining == false
               stamcheck = false
            end
            
            if vest.Parent == plr.Backpack and vest.Parent ~= plr.Character and not plr.Character:FindFirstChild(vestN):IsA("Model") and plr.Character:FindFirstChild(shakeN) == nil then
               plr.Character.Humanoid:EquipTool(plr.Backpack:FindFirstChild(vestN))
               task.wait(1)
                plr.Character:FindFirstChild(vestN):Activate()
                task.wait(0.5)
            elseif plr.Character:FindFirstChild(vestN) and plr.Character:FindFirstChild(vestN):IsA("Model") and plr.Character:FindFirstChild(shakeN) == nil then
                if weight.Parent == plr.Backpack and plr.Character:FindFirstChild(shakeN) == nil then
                   plr.Character.Humanoid:EquipTool(weight)
                   task.wait(1)
                elseif weight.Parent == plr.Character and plr.Character:FindFirstChild(shakeN) == nil then
                    weight:Activate()
                end
            end
        end)
    end
end
end
end

HighVestTrainFunc = function()
    game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
    task.wait()
    for i,v in next, game.Players.LocalPlayer.Backpack:GetChildren() do
    if v:isA("Tool") and v.Name == "50KG Vest" then
        game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
        game:GetService("Players").LocalPlayer.Character:FindFirstChild("50KG Vest"):Activate()
        game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
        task.wait()

    local vestN = "50KG Vest"
    local shakeN = "Vanilla Protein Shake"
    local weightN = "40KG Dumbbell"
    
    local plr = game.Players.LocalPlayer
    
    while HighVestTraining and task.wait() do
        pcall(function()
            vest = plr.Backpack:FindFirstChild(vestN) or plr.Character:FindFirstChild(vestN)
            shake = plr.Backpack:FindFirstChild(shakeN) or plr.Character:FindFirstChild(shakeN)
            weight = plr.Backpack:FindFirstChild(weightN) or plr.Character:FindFirstChild(weightN)
        
            if StaminaCheck() then
               stamcheck = true
               repeat
                   local check = plr.Character:FindFirstChild("Stamina").Value
                   local MaxStamina = plr.Character:FindFirstChild("Stamina"):GetAttribute("Max")
                   task.wait()
               until check >= MaxStamina or HighVestTraining == false
               stamcheck = false
            end
            
            if vest.Parent == plr.Backpack and vest.Parent ~= plr.Character and not plr.Character:FindFirstChild(vestN):IsA("Model") and plr.Character:FindFirstChild(shakeN) == nil then
               plr.Character.Humanoid:EquipTool(plr.Backpack:FindFirstChild(vestN))
               task.wait(1)
                plr.Character:FindFirstChild(vestN):Activate()
                task.wait(0.5)
            elseif plr.Character:FindFirstChild(vestN) and plr.Character:FindFirstChild(vestN):IsA("Model") and plr.Character:FindFirstChild(shakeN) == nil then
                if weight.Parent == plr.Backpack and plr.Character:FindFirstChild(shakeN) == nil then
                   plr.Character.Humanoid:EquipTool(weight)
                   task.wait(1)
                elseif weight.Parent == plr.Character and plr.Character:FindFirstChild(shakeN) == nil then
                    weight:Activate()
                end
            end
        end)
    end
end
end 
end

AutoEatFunc = function()
    while AutoEat and task.wait() do
        shake = plr.Backpack:FindFirstChild("Vanilla Protein Shake") or plr.Character:FindFirstChild("Vanilla Protein Shake") or nil
        
        if shake ~= nil then
            if shake.Parent == plr.Backpack and stamcheck == false then
              plr.Character.Humanoid:UnequipTools()
              shake.Parent = plr.Character
              task.wait()
            elseif shake.Parent == plr.Character and stamcheck == false then
                shake:Activate()
                task.wait(4)
                plr.Character.Humanoid:UnequipTools()
                task.wait(20)
            end
        end
    end
end

StreamerModeFunc = function()
    local plr = game.Players.LocalPlayer
    local char = plr.Character
    for i,v in pairs(char.Head:GetDescendants()) do
        if v:isA("TextLabel") then
             v.Text = ""
        end
    end
    
    for i,v in pairs(char:GetDescendants()) do
        if v:IsA("Clothing") or v:IsA("ShirtGraphic") then
            v:Destroy()
        end
    end
    
    local client = game:GetService("Players").LocalPlayer
    local char = client.Character
    
    for _,delete in pairs(char:GetChildren()) do
        if delete:IsA('Accessory') then
            delete:Destroy()
        end
    end
    
    for i,v in pairs(char.FakeHead:GetDescendants()) do
        if v:IsA("Decal") or v:IsA("Mesh") then
            v.Texture = "Bruh"
    
        repeat wait() until game:IsLoaded()
    getgenv().settings = {
        ['HiddenName'] = "?"
    }
    
    local Player = game.Players.LocalPlayer
    local function HideName(oldName)
        for Index, Value in next, game:GetDescendants() do 
            if Value.ClassName == "TextLabel" or Value.ClassName == "TextButton" then 
                
                local str = Value.Text:gsub(oldName,settings.HiddenName)
                str = str:gsub(Player.Name,settings.HiddenName)
                str = str:gsub(Player.DisplayName, settings.HiddenName)
                Value.Text = str 
                Value:GetPropertyChangedSignal("Text"):Connect(function()
                    local str = Value.Text:gsub(Player.Name,settings.HiddenName)
                    str = str:gsub(Player.DisplayName, settings.HiddenName)
                    Value.Text = str 
                end)
            end
        end
    end
    
    
    HideName(Player.Name)
    
    
    game.DescendantAdded:Connect(function(Value)
        if Value.ClassName == "TextLabel" or Value.ClassName == "TextButton" then 
            
            local str = Value.Text:gsub(Player.Name, settings.HiddenName)
            str = str:gsub(Player.DisplayName, settings.HiddenName)
            Value.Text = str 
            Value:GetPropertyChangedSignal("Text"):Connect(function()
                local str = Value.Text:gsub(Player.Name,settings.HiddenName)
                str = str:gsub(Player.DisplayName, settings.HiddenName)
                Value.Text = str 
            end)
    
        end
    end)
    end
    end
    
    InfRunFunc = function()
        while InfRun and task.wait() do
            local args = {
                [1] = "Running",
                [2] = false
            }
                
            game:GetService("ReplicatedStorage").Communication.Events.RunEvent:FireServer(unpack(args))
        end 
    end
end

AutoWithdrawFunc = function()
    while AutoWithdraw do task.wait(2.5)
        local args = {
            [1] = "DepositWithdraw",
            [2] = 1000,
            [3] = "Withdraw"
        }
        
        game:GetService("ReplicatedStorage").Communication.Functions.BankingRemote:InvokeServer(unpack(args))
    end
end

StuckPlayerFunc = function()
    char.Parent = nil
    char.HumanoidRootPart:Destroy()
    char.Parent = workspace 
end

local function RunningFunc()
    pcall(function()
    local plr = game.Players.LocalPlayer
local char = plr.Character
char:SetAttribute("Running", true)
end)
end

AutoLegitRoadworkFunc = function()
task.spawn(function()
while AutoLegitRoadwork do task.wait()
    RunningFunc()
end
end)
task.spawn(function()
    while AutoLegitRoadwork do task.wait()
    local PathfindingService = game:GetService("PathfindingService")
local function pathto(part)
    settings = {
        AgentRadius = 2;
        AgentHeight = 5;
        AgentCanJump = true; --Doesn't jump for some reason no matter what dont ask me.
    }
    local path = PathfindingService:CreatePath(settings)
    path:ComputeAsync(game.Players.LocalPlayer.Character.HumanoidRootPart.Position, part)
    local waypoints = path:GetWaypoints()
    for _, waypoint in pairs(waypoints) do
        local part = Instance.new("Part")
        part.Shape = "Ball"
        part.Material = "Neon"
        part.Size = Vector3.new(0.6, 0.6, 0.6)
        part.Position = waypoint.Position
        part.Anchored = true
        part.CanCollide = false
        part.Parent = game.Workspace
        game.Players.LocalPlayer.Character.Humanoid:MoveTo(waypoint.Position)
        game.Players.LocalPlayer.Character.Humanoid.MoveToFinished:Wait()
        part:Destroy()
pathto(Vector3.new(414, 97, 2430))
game:GetService("ReplicatedStorage").Communication.Events.Roadwork:FireServer()
task.wait(1)
pathto(Vector3.new(442.3219909667969, 101.93951416015625, 2410.8359375))
task.wait(0.5)
pathto(Vector3.new(452.335938, 101.186859, 2776.46826))
task.wait(0.5)
pathto(Vector3.new(143.901886, 101.939713, 2778.29102))
task.wait(0.5)
pathto(Vector3.new(134.294312, 101.186859, 2423.06299))
task.wait(0.5)
pathto(Vector3.new(368.769897, 101.186859, 2403.98804))
    end
end
end
end)
end

